#include <iostream>
using namespace std;

void main()
{
	float num1, num2;

	cout << " type in two floating points then press enter" << endl;
	cout << "n1 -> ";
		cin >> num1;
		cout << "n2 -> ";
		cin >> num2;

	cout << "n1 + n2 = " << num1 + num2 << endl;
	cout << "n1 - n2 = " << num1 - num2 << endl;
	cout << "n1 * n2 = " << num1 * num2 << endl;
	cout << "n1 / n2 = " << num1 / num2 << endl;

}
